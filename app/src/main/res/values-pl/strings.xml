	<string name="setText_summary_on">"Tłumaczenie tekstu napisanego przy użyciu set text - Safe. Wyłącz, aby wyłączyć."</string>
	<string name="setHint">"Przetłumacz wskazówki tekstowe (sugestie w kolorze szarym) w polach tekstowych"</string>
